<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.osxm.jcodef.func.mybatis.mapper.ChartMapper">
	<select id="getChartById" parameterType="int" resultType="cn.osxm.jcodef.func.mybatis.model.Chart">
		select * from base_chart where id=#{id}
	</select>

	<insert id="insertChart" parameterType="cn.osxm.jcodef.func.mybatis.model.Chart">
		insert into base_chart(name, description)
		values( #{name}, #{description})
	</insert>
</mapper>